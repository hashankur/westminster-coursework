<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #14121D;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.w03.ScannerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="inputText" />
   <HBox>
      <children>
          <Button onAction="#onScannerGetButtonClick" text="Get" />
         <Button onAction="#onScannerPutButtonClick" text="Put">
            <HBox.margin>
               <Insets left="50.0" />
            </HBox.margin>
         </Button>
      </children>
   </HBox>
</VBox>
